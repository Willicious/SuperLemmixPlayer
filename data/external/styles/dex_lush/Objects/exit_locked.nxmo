EFFECT LOCKEDEXIT
TRIGGER_X 19
TRIGGER_Y 31
TRIGGER_WIDTH 2
TRIGGER_HEIGHT 10

SOUND_EXHAUST slurp

$PRIMARY_ANIMATION
  NAME *blank
  FRAMES 16
  WIDTH 40
  HEIGHT 40
$END


$ANIMATION
  NAME opening
  FRAMES 16
  STATE MatchPhysics
  
  $TRIGGER
    CONDITION Exhausted
    HIDE
  $END
$END


$ANIMATION
  NAME constant
  OFFSET_X 10
  FRAMES 8
  HIDE
  
  $TRIGGER
    CONDITION Ready
  $END
  
  $TRIGGER
    CONDITION Exhausted
    HIDE
  $END
$END


$ANIMATION
  NAME closing
  FRAMES 12
  INITIAL_FRAME 1
  HIDE
  
  $TRIGGER
    CONDITION Exhausted
    STATE LoopToZero
  $END
$END

