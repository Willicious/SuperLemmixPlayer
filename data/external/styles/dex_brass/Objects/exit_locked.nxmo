EFFECT LOCKEDEXIT
TRIGGER_X 17
TRIGGER_Y 28
TRIGGER_WIDTH 2
TRIGGER_HEIGHT 10

SOUND_EXHAUST thunk

$PRIMARY_ANIMATION
  NAME *blank
  FRAMES 11
  WIDTH 36
  HEIGHT 36
$END


$ANIMATION
  NAME opening
  FRAMES 11
  STATE MatchPhysics
  
  $TRIGGER
    CONDITION Ready
    HIDE
  $END
  
  $TRIGGER
    CONDITION Exhausted
    HIDE
  $END
$END


$ANIMATION
  NAME main
  FRAMES 2
  HIDE
  
  $TRIGGER
    CONDITION Ready
  $END
  
  $TRIGGER
    CONDITION Exhausted
    HIDE
  $END
$END


$ANIMATION
  NAME closing
  FRAMES 15
  INITIAL_FRAME 1
  HIDE
  
  $TRIGGER
    CONDITION Exhausted
    STATE LoopToZero
  $END
$END

