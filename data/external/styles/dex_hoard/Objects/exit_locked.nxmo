    CONDITION Exhausted
    HIDE
  $END
$END


$ANIMATION
  NAME main
  FRAMES 8
  HIDE
  
  $TRIGGER
    CONDITION Ready
  $END
  
  $TRIGGER
    CONDITION Exhausted
    HIDE
  $END
$END


$ANIMATION
  NAME closing
  FRAMES 11
  INITIAL_FRAME 1
  HIDE
  
  $TRIGGER
    CONDITION Exhausted
    STATE LoopToZero
  $END
$END

