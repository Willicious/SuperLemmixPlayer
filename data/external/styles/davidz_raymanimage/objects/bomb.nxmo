  FRAMES 8
  OFFSET_X 15
  OFFSET_Y 15
  INITIAL_FRAME RANDOM
  Z_INDEX 2

  $TRIGGER
    CONDITION DISABLED
    STATE LOOPTOZERO
  $END
  
