$END

$STATE_RECOLORING
  
  $ATHLETE
    FROM x00B000
    TO xCF2020
  $END
  
  $ATHLETE
    FROM xD02020
    TO x00AF00
  $END
  
  $ZOMBIE
    FROM xF0D0D0
    TO x808080
  $END
  
